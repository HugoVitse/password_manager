{
    "manifest_version": 2,
    "name": "modify-page",
    "version": "1.0",
  
    "permissions": ["activeTab", "webNavigation","tabs"],
  
    "background": {
      "scripts": ["background.js"]
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content-script.js"]
        }
    ]
  }
  